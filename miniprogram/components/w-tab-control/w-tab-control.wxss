/* components/w-tab-control/w-tab-control.wxss */
.tab-control {
  display: flex;
  height: 40px;
  line-height: 40px;
  background: #fcfcfc;
}

.item {
  flex: 1;
  text-align: center;
  font-size: 30rpx;
}

.item text {
  padding: 0 5px 7px;
}

.item .active {
  border-bottom: 2px solid #d33b33;
  color: #d33b33;
}
